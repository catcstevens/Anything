<h1>Neighbour</h1>

<p>Name: <%= full_name %></p>
<p>Name: <%= @user.username %></p>
<p>About: <%= @user.about %></p>
<p>Location: <%= @user.location %></p>
<p>Delivers? <%= @user.delivers %></p>

<% @user_listings.each do |listing| %>
<%= image_tag listing.picture if listing.picture.attached? %>
<%= link_to listing.title, listings_path(listing) %>

<button type="button" class="btn btn-custom"><%= link_to "Edit profile", edit_user_registration_path if current_user.id == listing.user.id %></button >

    <% end %>